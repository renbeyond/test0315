<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="200dp"
    android:width="200dp"
    android:viewportHeight="100"
    android:viewportWidth="100">
    <path android:name="circle"
        android:strokeColor="@color/colorAccent"
        android:strokeWidth="2"
        android:pathData="M 50,50 m -48,0 a 48,48 0 1,0 96,0a 48,48 0 1,0 -96,0"/>

    <group android:name="seconds"
        android:pivotX="50"
        android:pivotY="50"
        android:rotation="0">
        <path android:strokeColor="@color/colorPrimary"
            android:strokeWidth="3"
            android:strokeLineCap="round"
            android:pathData="M50,50 L50,10"/>
    </group>

    <group android:name="minutes"
        android:pivotX="50"
        android:pivotY="50"
        android:rotation="90">
        <path android:strokeWidth="4"
            android:strokeColor="@color/colorPrimaryDark"
            android:strokeLineCap="round"
            android:pathData="M50,50 L50,20"/>
    </group>
</vector>